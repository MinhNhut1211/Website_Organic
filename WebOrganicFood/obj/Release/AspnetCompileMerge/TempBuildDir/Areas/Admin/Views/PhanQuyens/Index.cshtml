@model IEnumerable<WebOrganicFood.Models.PhanQuyen>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.IDQuyen)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TenQuyen)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.IDQuyen)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TenQuyen)
        </td>
        <td>
            @Html.ActionLink("Sửa", "Edit", new { id = item.IDQuyen }) |
            @Html.ActionLink("Xem", "Details", new { id = item.IDQuyen }) |
            @Html.ActionLink("Xóa", "Delete", new { id = item.IDQuyen })
        </td>
    </tr>
}

</table>
